  "main": "/dist/src/components/Calendar/CalendarWrapper.js",
  "types": "/dist/src/components/Calendar/Calendar.types.d.ts",
  "keywords": ["react-calendar", "react-timeline-calendar", "react-event-calendar", "react-google-calendar"],
  "author": "Jovan Jenjic",
  "license": "MIT",
  "dependencies": {
